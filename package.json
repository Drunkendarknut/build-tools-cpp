{
  "name": "build-tools",
  "main": "./lib/main",
  "version": "4.3.0",
  "description": "Build your projects in atom",
  "repository": "https://github.com/deprint/build-tools-cpp",
  "license": "MIT",
  "scripts": {
    "lint": "coffeelint lib"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "pty.js": "^0.3.0",
    "atom-space-pen-views": "^2.0.0",
    "fs-plus": "^2.0.4",
    "season": "^5.2.0",
    "xregexp": "^2.0.0"
  },
  "keywords": [
    "build",
    "compile",
    "tools",
    "projects"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    },
    "build-tools.input": {
      "versions": {
        "1.0.0": "provideInput"
      }
    },
    "build-tools.console": {
      "versions": {
        "1.0.0": "provideConsole"
      }
    }
  },
  "consumedServices": {
    "build-tools.modifier": {
      "versions": {
        "^1.0.0": "consumeModifierModule"
      }
    },
    "build-tools.profile": {
      "versions": {
        "^1.0.0": "consumeProfileModule"
      }
    },
    "build-tools.provider": {
      "versions": {
        "^1.0.0": "consumeProviderModule"
      }
    },
    "build-tools.output": {
      "versions": {
        "^1.0.0": "consumeOutputModule"
      }
    }
  }
}
