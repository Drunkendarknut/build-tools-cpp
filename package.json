{
  "name": "build-tools",
  "main": "./lib/main",
  "version": "3.6.1",
  "description": "Build your projects in atom",
  "repository": "https://github.com/deprint/build-tools-cpp",
  "license": "MIT",
  "scripts": {
    "lint": "coffeelint lib"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0",
    "fs-plus": "^2.0.4",
    "season": "^5.2.0",
    "xregexp": "^2.0.0"
  },
  "keywords": [
    "build",
    "compile",
    "tools",
    "projects"
  ],
  "specHelper": "./spec/helper",
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "consumedServices": {
    "build-tools.profile": {
      "versions": {
        "^1.0.0": "consumeProfile"
      }
    }
  }
}
