{
  "name": "build-tools",
  "main": "./lib/main",
  "version": "3.0.1",
  "description": "Build your projects in atom",
  "repository": "https://github.com/deprint/build-tools-cpp",
  "activationCommands": {
    "atom-workspace": [
      "build-tools:first-command",
      "build-tools:second-command",
      "build-tools:third-command",
      "build-tools:show",
      "build-tools:settings",
      "build-tools:commands"
    ]
  },
  "license": "MIT",
  "engines": {
    "atom": ">=0.197.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0",
    "fs-plus": "^2.0.4",
    "season": "^5.2.0",
    "xregexp": "^2.0.0"
  },
  "keywords": [
    "build",
    "compile",
    "tools",
    "projects"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
