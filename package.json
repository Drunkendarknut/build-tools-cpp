{
  "name": "build-tools-cpp",
  "main": "./lib/build-tools-cpp",
  "linter-package": true,
  "linter-implementation": "build-lint",
  "version": "1.2.6",
  "description": "Build your autotools / CMake / Makefile project in atom",
  "repository": "https://github.com/deprint/build-tools-cpp",
  "activationCommands": {
    "atom-workspace": [
      "build-tools-cpp:pre-configure",
      "build-tools-cpp:configure",
      "build-tools-cpp:make",
      "build-tools-cpp:toggle",
      "build-tools-cpp:settings",
      "build-tools-cpp:show-commands"
    ]
  },
  "license": "MIT",
  "engines": {
    "atom": ">=0.197.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^1.0.0",
    "fs-plus": "^2.0.4"
  }
}
